{{#each this}}
<li class="list__element">
    <img class="element__photo" src="https://image.tmdb.org/t/p/original{{poster_path}}" data-src="{{id}}" alt="" />
    <h4 class="element__name">{{original_title}}{{original_name}}</h4>
    <div class="element__genres-and-rating">
        <p class="element__genres">{{genre_names}} | {{release_date}}</p>
        <p class="element__rating">{{vote_average}}</p>
    </div>
</li>
{{/each}}